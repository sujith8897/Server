
<%- include('partials/header');-%>

  <section id="content-wrapper" class="container">
      <div class="row" >
          <div class="col">

          <h3 style="text-align:center;">Issues</h3>
            <% if(data.length>0) {%>
                  <table>
                    <tr>
                      <th>Roll</th>
                      <th>Block</th>
                      <th>Room</th>
                      <th>Issue</th>
                      <th>Remove</th>
                    </tr>
                  <% for(var i=0;i<data.length;i++){ %>
                      <tr>
                        <td><a href="<%=data[i].roll%>"><%= data[i].roll %></a></td>
                        <td><%= data[i].blockName %></td>
                        <td><%= data[i].room %></td>
                        <td><%= data[i].issue %></td>
                        <td><a href="/delIssue/<%= data[i]._id %>"><i class="fa fa-trash-o icon"></i></a></td>
                      </tr>
                      <% }%>
                  </table>
            <% }else { %>
                <h3>No issues to show</h3>
                <a href="/dashboard"><button class="btn btn-warning">Home</button></a>

            <% } %>  
          </div>
      </div>
  </section>

<%- include('partials/footer');-%>


